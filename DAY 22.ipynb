   "source": [
    "import spacy\n",
    "\n",
    "# Load the English NLP model\n",
    "nlp = spacy.load('en_core_web_sm')\n",
    "\n",
    "# Define the text\n",
    "txt = 'NLP is amazing and fun to learn.'\n",
    "\n",
    "# Process the text using SpaCy\n",
    "doc = nlp(txt)\n",
    "\n",
    "# Print part-of-speech tags\n",
    "for token in doc:\n",
    "    print(f'{token.text}: {token.pos_}')\n"
   ]
